{
  "name": "workspace",
  "scripts": {
    "elm:build": "elm make src/Main.elm --output=./dist/elm.js",
    "elm:review": "elm-review",
    "elm:test": "elm-test",
    "tailwind": "tailwindcss -i ./style.css -o ./dist/style.css --watch",
    "tailwind:snap": "tailwindcss -i ./style.css -o ./dist/style.css --watch",
    "start": "vite -c ./vite.config.js",
    "check": "npm run elm:build && npm run elm:review && npm run elm:test"
  },
  "devDependencies": {
    "elm-format": "^0.8.5",
    "elm-review": "^2.6.1",
    "elm-test": "^0.19.1-revision7",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.7",
    "vite": "^2.7.7",
    "vite-plugin-elm": "^2.5.1"
  },
  "dependencies": {
    "elm": "^0.19.1-5"
  }
}